#include<stdio.h>
void bubbleAsc(int [], int);
void bubbleDesc(int [], int);
void bubbleAsc(int X[],int n)
{
	for (int i=0; i<SIZE-1; i++)
{
	int tmp;
	for (int j=0; j<SIZE-i-1; j++)
	{
		if (X[j] > X[j+1])
		{
		tmp = X[j];
		X[j] = X[j+1];
		X[j+1] = tmp;
		}
	}
 }
}
void bubbleDesc(int X[],int n)
{
	for (int i=0; i<SIZE-1; i++)
{
	int tmp;
	for (int j=0; j<SIZE-i-1; j++)
	{
		if (X[j] < X[j+1])
		{
		tmp = X[j];
		X[j] = X[j+1];
		X[j+1] = tmp;
		}
	}



   
